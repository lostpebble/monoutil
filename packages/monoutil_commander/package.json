{
  "name": "@monoutil/commander",
  "version": "1.0.0",
  "description": "A locally run commander UI to work with packages and monorepos on your local machine.",
  "type": "module",
  "scripts": {
    "dev:server": "bun --hot src/server.ts",
    "dev:web": "bunx vite --config web/vite.config.ts",
    "dev": "bunx concurrently \"bun run dev:server\" \"bun run dev:web\"",
    "build:web": "bunx vite build --config web/vite.config.ts",
    "start": "bun src/server.ts"
  },
  "dependencies": {
    "@hono/cors": "^4.6.7",
    "hono": "^4.6.7",
    "local-package-link": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "bun-types": "^1.1.29",
    "concurrently": "^9.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite": "^5.4.2",
    "@vitejs/plugin-react": "^4.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lostpebble/monorepo-utils.git"
  },
  "private": true
}
